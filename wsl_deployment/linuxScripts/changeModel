#this script runs on the linux system
#if a subdirectory named "model" does not exist, create it.
#rm any existing .onnx* files in that subdirectory
#wget the URL on the command line along with 
#wget of the URL with ".json" appended.
cd "$(dirname "$0")"
[ ! -d model ] && mkdir model
cd model
if [ -n "$1" ]; then
    ls *.onnx* 1>/dev/null 2>&1
    [ $? -eq 0 ] && rm *.onnx*
    wget -nv -t 1 "$1"
    [ $? -ne 0 ] && echo failed && exit 1
    wget -nv -t 1 "$1".json
else
    echo "No URL specified" >&2
fi
